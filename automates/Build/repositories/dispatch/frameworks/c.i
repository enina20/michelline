use google_storage1::Storage;
use once_cell::sync::Lazy;
use std::collections::HashMap;
use std::io::Cursor;

static GIT_HASH: Lazy<String> = Lazy::new(|| {
  test_util::run_collect(&["git", "rev-parse", "HEAD"], None, None, None, true)
    .0
    .trim()
    .to_string()
